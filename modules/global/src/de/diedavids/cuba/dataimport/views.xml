<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="de.diedavids.cuba.dataimport.entity.ImportExecution"
          extends="_local"
          name="importExecution-with-records-view">
        <property name="file"
                  view="_minimal"/>
        <property name="records"
                  view="_local"/>
        <property name="configuration"
                  view="_minimal"/>
    </view>
    <view class="de.diedavids.cuba.dataimport.entity.ImportExecution"
          extends="_local"
          name="importExecution-view">
        <property name="file"
                  view="_minimal"/>
        <property name="configuration"
                  view="_minimal"/>
    </view>
    <view class="de.diedavids.cuba.dataimport.entity.ImportConfiguration"
          extends="_local"
          name="importConfiguration-view">
        <property name="logs"
